# Minecraft Server Properties

# This is the same as 'server.properties'.
# The Minecraft Server automatically overwrites that file every run.
# Make sure to overwrite that file via your launcher for changes to take effect.
# This file allows the operator to keep settings organized.

# Performance
view-distance=16
entity-broadcast-range-percentage=100
max-tick-time=60000
sync-chunk-writes=false
enable-jmx-monitoring=false

# World Generation
level-name=NewWorld
level-type=largebiomes
level-seed=
generate-structures=true
generator-settings=
max-world-size=29999984

# Game
gamemode=adventure
force-gamemode=false
pvp=false
difficulty=easy
hardcore=false
spawn-npcs=true
spawn-animals=true
spawn-monsters=true
allow-flight=false
allow-nether=true
max-build-height=256
spawn-protection=32
enable-command-block=true

# Server Identity
# motd=Now Running On A 'Thicc Boi Over There' Virtual Machine
# motd=Development Mode : Changes May Not Be Saved
# motd=Smol Boi Server For 1 To 2 Players
enable-status=true
enable-query=true
query.port=25565

# Server-Side Resource Pack
resource-pack=
resource-pack-sha1=

# Permissions
white-list=true
enforce-whitelist=true
online-mode=true
prevent-proxy-connections=false
function-permission-level=4
op-permission-level=4
broadcast-console-to-ops=false
player-idle-timeout=10
rate-limit=0

# Network
server-ip=
